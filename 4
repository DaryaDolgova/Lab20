using namespace std;

#include <string>
#include <iostream>

int main() 
{
	char c;
	string s;

	cout << "srtoka=";
	getline(cin, s);
	cout << "simvol=";
	cin >> c;

	for (int i = s.size(); i >=0; i--) 
	{
		if (s[i]==c) { s.insert(i, 1, c); }
	}

	cout << "poluchivshaysy srtoka=" << s << endl;

	return 0;
}
