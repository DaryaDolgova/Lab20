using namespace std;

#include <string>
#include <iostream>

int main() 
{
	string S, S0;
	int x, n, z;
	x=0; 
	n=0;
	z=0;
	cout << "S= ";
	getline(cin, S);
	cout << "S0= ";
	getline(cin, S0);

	for (int i = 0; i <=S.size(); i++) 
	{
		if (S[i] == S0[z]) 
		{
			x++;
			z++;
			if (x == S0.size()) { n++; }
		}
				else { x = 0; z = 0; }
	}

	cout << "kollichestvo = " << n << endl;

	return 0;
}
